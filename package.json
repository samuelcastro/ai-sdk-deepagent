  "description": "Deep Agents implementation using Vercel AI SDK - build controllable AI agents with planning, filesystem, and subagent capabilities",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "type": "module",
  "bin": {
    "deep-agent": "./src/cli/index.tsx"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./cli": {
      "import": "./src/cli/index.ts"
    }
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run typecheck",
    "cli": "bun src/cli/index.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrispangg/ai-sdk-deepagent.git"
  },
  "homepage": "https://github.com/chrispangg/ai-sdk-deepagent#readme",
  "bugs": {
    "url": "https://github.com/chrispangg/ai-sdk-deepagent/issues"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.0",
    "@ai-sdk/openai": "^3.0.0-beta.70",
    "@ai-sdk/react": "^3.0.0-beta.120",
    "@inkjs/ui": "^1.0.0",
    "ai": "^6.0.0-beta.120",
    "fast-glob": "^3.3.3",
    "ink": "^5.1.0",
    "micromatch": "^4.0.8",
    "react": "^18.2.0",
    "react-devtools-core": "^5.3.2",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
